<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="12dip" />
            <stroke android:width="1dip" android:color="#617c8d" />
            <gradient android:angle="-90" android:startColor="#617c8d" android:endColor="#689a92"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="12dip" />
            <stroke android:width="1dip" android:color="#617c8d" />
            <solid android:color="#617c8d"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="12dip" />
            <stroke android:width="1dip" android:color="#617c8d" />
            <gradient android:angle="-90" android:startColor="#617c8d" android:endColor="#617c8d" />
        </shape>
    </item>
</selector>